  - title: child1 Chores
    path: child1 Chores
    subview: true
    cards:
      - square: false
        type: grid
        columns: 2
        cards:
          - type: custom:button-card
            entity: input_boolean.child1_chore_01_enabled
            show_name: true
            show_icon: true
            show_state: false
            tap_action:
              action: navigate
              navigation_path: child1 Chore 01
            hold_action:
              action: none
            name: Chore 01
            icon: |
              [[[
                if (states['input_boolean.child1_chore_01_enabled'].state === 'off') {
                  return 'mdi:text-box';
                } else if (states['input_boolean.child1_chore_01_pending'].state === 'off' &&
                          states['input_boolean.child1_chore_01_completed'].state === 'off') {
                  return 'mdi:text-box-outline';
                } else if (states['input_boolean.child1_chore_01_pending'].state === 'on' &&
                          states['input_boolean.child1_chore_01_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child1_chore_01_pending'].state === 'on' &&
                          states['input_boolean.child1_chore_01_completed'].state === 'on') {
                  return 'mdi:text-box-check-outline';
                } else {
                  return 'mdi:alert'; // Default icon for unexpected cases
                }
              ]]]
            custom_fields:
              label_top: |
                [[[
                  return states['input_text.child1_chore_01_chore'].state;
                ]]]
              label_bottom: |
                [[[
                  return parseInt(states['input_number.child1_chore_01_chore_value'].state,) + ' pts';
                ]]]
              days_row: |
                [[[
                  let days = {
                    Sun: states['input_boolean.child1_chore_01_sunday'].state,
                    Mon: states['input_boolean.child1_chore_01_monday'].state,
                    Tue: states['input_boolean.child1_chore_01_tuesday'].state,
                    Wed: states['input_boolean.child1_chore_01_wednesday'].state,
                    Thr: states['input_boolean.child1_chore_01_thursday'].state,
                    Fri: states['input_boolean.child1_chore_01_friday'].state,
                    Sat: states['input_boolean.child1_chore_01_saturday'].state,
                  };
                  return Object.entries(days).map(([key, value]) =>
                    `<span style="color: ${value === 'on' ? 'green' : 'red'}">${key}</span>`).join(' ');
                ]]]
            styles:
              card:
                - padding: 10px
                - text-align: center
                - display: flex
                - flex-direction: column
                - justify-content: center
                - align-items: center
                - height: 150px
              custom_fields:
                label_top:
                  - text-align: left
                  - font-size: 18px
                  - color: |
                      [[[
                        if (states['input_boolean.child1_chore_01_enabled'].state === 'off') {
                          return 'grey';
                        } else if (states['input_boolean.child1_chore_01_pending'].state === 'on' &&
                                  states['input_boolean.child1_chore_01_completed'].state === 'off') {
                          return 'blue';
                        } else if (states['input_boolean.child1_chore_01_pending'].state === 'on' &&
                                  states['input_boolean.child1_chore_01_completed'].state === 'on') {
                          return 'green';
                        } else {
                          return 'orange';
                        }
                      ]]]
                  - margin-bottom: 10px
                  - position: absolute
                  - top: 110px
                label_bottom:
                  - text-align: left
                  - font-size: 16px
                  - color: |
                      [[[
                        if (states['input_boolean.child1_chore_01_enabled'].state === 'off') {
                          return 'grey';
                        } else if (states['input_boolean.child1_chore_01_pending'].state === 'on' &&
