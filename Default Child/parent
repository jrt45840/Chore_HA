            show_state: false
            tap_action:
              action: navigate
              navigation_path: child4 Chore 03
            hold_action:
              action: none
            name: Chore 03
            icon: |
              [[[
                if (states['input_boolean.child4_chore_03_enabled'].state === 'off') {
                  return 'mdi:text-box';
                } else if (states['input_boolean.child4_chore_03_pending'].state === 'off' &&
                          states['input_boolean.child4_chore_03_completed'].state === 'off') {
                  return 'mdi:text-box-outline';
                } else if (states['input_boolean.child4_chore_03_pending'].state === 'on' &&
                          states['input_boolean.child4_chore_03_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child4_chore_03_pending'].state === 'on' &&
                          states['input_boolean.child4_chore_03_completed'].state === 'on') {
                  return 'mdi:text-box-check-outline';
                } else {
                  return 'mdi:alert'; // Default icon for unexpected cases
                }
              ]]]
            custom_fields:
              label_top: |
                [[[
                  return states['input_text.child4_chore_03_chore'].state;
                ]]]
              label_bottom: |
                [[[
                  return parseInt(states['input_number.child4_chore_03_chore_value'].state,) + ' pts';
                ]]]
              days_row: |
                [[[
                  let days = {
                    Sun: states['input_boolean.child4_chore_03_sunday'].state,
                    Mon: states['input_boolean.child4_chore_03_monday'].state,
                    Tue: states['input_boolean.child4_chore_03_tuesday'].state,
