                [[[
                  let days = {
                    Sun: states['input_boolean.child4_chore_11_sunday'].state,
                    Mon: states['input_boolean.child4_chore_11_monday'].state,
                    Tue: states['input_boolean.child4_chore_11_tuesday'].state,
