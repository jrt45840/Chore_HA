                          states['input_boolean.child5_chore_01_completed'].state === 'off') {
                  return 'mdi:text-box-outline';
                } else if (states['input_boolean.child5_chore_01_pending'].state === 'on' &&
                          states['input_boolean.child5_chore_01_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child5_chore_01_pending'].state === 'on' &&
                          states['input_boolean.child5_chore_01_completed'].state === 'on') {
                  return 'mdi:text-box-check-outline';
                } else {
                  return 'mdi:alert'; // Default icon for unexpected cases
                }
              ]]]
            custom_fields:
              label_top: |
                [[[
                  return states['input_text.child5_chore_01_chore'].state;
                ]]]
              label_bottom: |
                [[[
                  return parseInt(states['input_number.child5_chore_01_chore_value'].state,) + ' pts';
                ]]]
              days_row: |
                [[[
                  let days = {
                    Sun: states['input_boolean.child5_chore_01_sunday'].state,
                    Mon: states['input_boolean.child5_chore_01_monday'].state,
                    Tue: states['input_boolean.child5_chore_01_tuesday'].state,
