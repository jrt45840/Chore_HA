                [[[
                  return parseInt(states['input_number.child4_chore_15_chore_value'].state,) + ' pts';
                ]]]
              days_row: |
                [[[
                  let days = {
                    Sun: states['input_boolean.child4_chore_15_sunday'].state,
                    Mon: states['input_boolean.child4_chore_15_monday'].state,
