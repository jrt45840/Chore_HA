                } else if (states['input_boolean.child1_chore_44_pending'].state === 'off' &&
                          states['input_boolean.child1_chore_44_completed'].state === 'off') {
                  return 'mdi:text-box-outline';
                } else if (states['input_boolean.child1_chore_44_pending'].state === 'on' &&
                          states['input_boolean.child1_chore_44_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child1_chore_44_pending'].state === 'on' &&
                          states['input_boolean.child1_chore_44_completed'].state === 'on') {
                  return 'mdi:text-box-check-outline';
                } else {
                  return 'mdi:alert'; // Default icon for unexpected cases
                }
