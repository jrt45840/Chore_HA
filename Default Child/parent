                  return 'mdi:text-box';
                } else if (states['input_boolean.child4_chore_15_pending'].state === 'off' &&
                          states['input_boolean.child4_chore_15_completed'].state === 'off') {
                  return 'mdi:text-box-outline';
                } else if (states['input_boolean.child4_chore_15_pending'].state === 'on' &&
                          states['input_boolean.child4_chore_15_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child4_chore_15_pending'].state === 'on' &&
                          states['input_boolean.child4_chore_15_completed'].state === 'on') {
                  return 'mdi:text-box-check-outline';
                } else {
                  return 'mdi:alert'; // Default icon for unexpected cases
                }
