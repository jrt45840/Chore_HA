                          states['input_boolean.child4_chore_11_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child4_chore_11_pending'].state === 'on' &&
                          states['input_boolean.child4_chore_11_completed'].state === 'on') {
