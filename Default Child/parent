              label_top: |
                [[[
                  return states['input_text.child3_chore_45_chore'].state;
                ]]]
              label_bottom: |
                [[[
                  return parseInt(states['input_number.child3_chore_45_chore_value'].state,) + ' pts';
                ]]]
              days_row: |
                [[[
                  let days = {
                    Sun: states['input_boolean.child3_chore_45_sunday'].state,
                    Mon: states['input_boolean.child3_chore_45_monday'].state,
