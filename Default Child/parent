            hold_action:
              action: none
            name: Chore 45
            icon: |
              [[[
                if (states['input_boolean.child3_chore_45_enabled'].state === 'off') {
                  return 'mdi:text-box';
                } else if (states['input_boolean.child3_chore_45_pending'].state === 'off' &&
                          states['input_boolean.child3_chore_45_completed'].state === 'off') {
                  return 'mdi:text-box-outline';
                } else if (states['input_boolean.child3_chore_45_pending'].state === 'on' &&
                          states['input_boolean.child3_chore_45_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child3_chore_45_pending'].state === 'on' &&
                          states['input_boolean.child3_chore_45_completed'].state === 'on') {
                  return 'mdi:text-box-check-outline';
                } else {
                  return 'mdi:alert'; // Default icon for unexpected cases
                }
              ]]]
