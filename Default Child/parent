          - type: custom:button-card
            entity: input_boolean.child3_chore_37_enabled
            show_name: true
            show_icon: true
            show_state: false
            tap_action:
              action: navigate
              navigation_path: child3 Chore 37
            hold_action:
              action: none
            name: Chore 37
            icon: |
              [[[
                if (states['input_boolean.child3_chore_37_enabled'].state === 'off') {
                  return 'mdi:text-box';
                } else if (states['input_boolean.child3_chore_37_pending'].state === 'off' &&
                          states['input_boolean.child3_chore_37_completed'].state === 'off') {
                  return 'mdi:text-box-outline';
                } else if (states['input_boolean.child3_chore_37_pending'].state === 'on' &&
                          states['input_boolean.child3_chore_37_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child3_chore_37_pending'].state === 'on' &&
                          states['input_boolean.child3_chore_37_completed'].state === 'on') {
                  return 'mdi:text-box-check-outline';
                } else {
                  return 'mdi:alert'; // Default icon for unexpected cases
                }
              ]]]
            custom_fields:
              label_top: |
                [[[
                  return states['input_text.child3_chore_37_chore'].state;
                ]]]
              label_bottom: |
                [[[
                  return parseInt(states['input_number.child3_chore_37_chore_value'].state,) + ' pts';
                ]]]
              days_row: |
                [[[
                  let days = {
                    Sun: states['input_boolean.child3_chore_37_sunday'].state,
                    Mon: states['input_boolean.child3_chore_37_monday'].state,
                    Tue: states['input_boolean.child3_chore_37_tuesday'].state,
