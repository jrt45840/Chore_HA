              label_top: |
                [[[
                  return states['input_text.child4_chore_07_chore'].state;
                ]]]
              label_bottom: |
                [[[
                  return parseInt(states['input_number.child4_chore_07_chore_value'].state,) + ' pts';
                ]]]
              days_row: |
                [[[
                  let days = {
                    Sun: states['input_boolean.child4_chore_07_sunday'].state,
                    Mon: states['input_boolean.child4_chore_07_monday'].state,
                    Tue: states['input_boolean.child4_chore_07_tuesday'].state,
