                } else if (states['input_boolean.child2_chore_02_pending'].state === 'off' &&
                          states['input_boolean.child2_chore_02_completed'].state === 'off') {
                  return 'mdi:text-box-outline';
                } else if (states['input_boolean.child2_chore_02_pending'].state === 'on' &&
                          states['input_boolean.child2_chore_02_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child2_chore_02_pending'].state === 'on' &&
                          states['input_boolean.child2_chore_02_completed'].state === 'on') {
                  return 'mdi:text-box-check-outline';
                } else {
                  return 'mdi:alert'; // Default icon for unexpected cases
                }
              ]]]
            custom_fields:
              label_top: |
                [[[
                  return states['input_text.child2_chore_02_chore'].state;
                ]]]
              label_bottom: |
                [[[
                  return parseInt(states['input_number.child2_chore_02_chore_value'].state,) + ' pts';
                ]]]
              days_row: |
                [[[
                  let days = {
                    Sun: states['input_boolean.child2_chore_02_sunday'].state,
                    Mon: states['input_boolean.child2_chore_02_monday'].state,
                    Tue: states['input_boolean.child2_chore_02_tuesday'].state,
