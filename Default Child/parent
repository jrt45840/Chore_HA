            show_state: false
            tap_action:
              action: navigate
              navigation_path: child1 Chore 36
            hold_action:
              action: none
            name: Chore 36
            icon: |
              [[[
                if (states['input_boolean.child1_chore_36_enabled'].state === 'off') {
                  return 'mdi:text-box';
                } else if (states['input_boolean.child1_chore_36_pending'].state === 'off' &&
                          states['input_boolean.child1_chore_36_completed'].state === 'off') {
                  return 'mdi:text-box-outline';
                } else if (states['input_boolean.child1_chore_36_pending'].state === 'on' &&
                          states['input_boolean.child1_chore_36_completed'].state === 'off') {
                  return 'mdi:text-box-remove-outline';
                } else if (states['input_boolean.child1_chore_36_pending'].state === 'on' &&
                          states['input_boolean.child1_chore_36_completed'].state === 'on') {
                  return 'mdi:text-box-check-outline';
                } else {
                  return 'mdi:alert'; // Default icon for unexpected cases
                }
              ]]]
            custom_fields:
              label_top: |
                [[[
                  return states['input_text.child1_chore_36_chore'].state;
                ]]]
              label_bottom: |
                [[[
                  return parseInt(states['input_number.child1_chore_36_chore_value'].state,) + ' pts';
                ]]]
              days_row: |
                [[[
                  let days = {
                    Sun: states['input_boolean.child1_chore_36_sunday'].state,
                    Mon: states['input_boolean.child1_chore_36_monday'].state,
                    Tue: states['input_boolean.child1_chore_36_tuesday'].state,
                    Wed: states['input_boolean.child1_chore_36_wednesday'].state,
