                entity_id: input_boolean.notify_2_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 69 to Device 2
                service: notify.device_2
                data:
                  title: Chore Completed
                  message: >-
                    Child1 has completed {{ states('input_text.child1_chore_69_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 69 Device 3
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_3_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 69 to Device 3
                service: notify.device_3
                data:
                  title: Chore Completed
                  message: >-
                    Child1 has completed {{ states('input_text.child1_chore_69_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 69 Device 4
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_4_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 69 to Device 4
                service: notify.device_4
                data:
                  title: Chore Completed
                  message: >-
                    Child1 has completed {{ states('input_text.child1_chore_69_chore') }}
                  data:
