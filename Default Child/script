        target:
          entity_id: input_boolean.child2_chore_91_pending

      - alias: Conditionally Send Notification for Chore 91 Device 1
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_1_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 91 to Device 1
                service: notify.device_1
                data:
                  title: Chore Completed
                  message: >-
                    Child2 has completed {{ states('input_text.child2_chore_91_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 91 Device 2
        choose:
