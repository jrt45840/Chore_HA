                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_confirm }}"
            sequence:
              - alias: Increment counter for Chore 97
                repeat:
                  count: "{{ states('input_number.child4_chore_97_chore_value') | int }}"
