        service: input_boolean.turn_on
        target:
          entity_id: input_boolean.child4_chore_61_pending

      - alias: Conditionally Send Notification for Chore 61 Device 1
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_1_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 61 to Device 1
                service: notify.device_1
                data:
                  title: Chore Completed
                  message: >-
                    Child4 has completed {{ states('input_text.child4_chore_61_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 61 Device 2
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_2_enabled
