        choose:
          - conditions:
              - condition: template
                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_confirm }}"
            sequence:
              - alias: Increment counter for Chore 11
                repeat:
                  count: "{{ states('input_number.child3_reward_11_reward_value') | int }}"
                  sequence:
                    - service: counter.increment
                      target:
                        entity_id: counter.child3_counter

              - alias: Mark Chore 
                service: input_boolean.turn_on
                target:
                  entity_id: input_boolean.child3_reward_11_claimed

          - conditions:
              - condition: template
                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_dismiss }}"
            sequence:
              - alias: Clear pending notification for Reward 11
                service: input_boolean.turn_off
                target:
                  entity_id: input_boolean.child3_reward_11_pending

          - conditions:
