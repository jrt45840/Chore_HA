                  entity_id: script.child5_chore_24_pending_completion_helper

  child5_chore_25_pending_completion:
    alias: Child5 Chore 25 Pending Completion
    sequence:
      - alias: Set up variables for Chore 25
        variables:
          action_confirm: "{{ 'CONFIRM_' ~ context.id }}"
          action_dismiss: "{{ 'DISMISS_' ~ context.id }}"

      - alias: Mark notification for Chore 25 as pending
        service: input_boolean.turn_on
        target:
          entity_id: input_boolean.child5_chore_25_pending

      - alias: Conditionally Send Notification for Chore 25 Device 1
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_1_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 25 to Device 1
                service: notify.device_1
                data:
                  title: Chore Completed
                  message: >-
                    Child5 has completed {{ states('input_text.child5_chore_25_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 25 Device 2
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_2_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 25 to Device 2
                service: notify.device_2
                data:
                  title: Chore Completed
                  message: >-
                    Child5 has completed {{ states('input_text.child5_chore_25_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 25 Device 3
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_3_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 25 to Device 3
                service: notify.device_3
                data:
                  title: Chore Completed
                  message: >-
                    Child5 has completed {{ states('input_text.child5_chore_25_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 25 Device 4
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_4_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 25 to Device 4
                service: notify.device_4
                data:
                  title: Chore Completed
                  message: >-
                    Child5 has completed {{ states('input_text.child5_chore_25_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 25 Device 5
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_5_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 25 to Device 5
                service: notify.device_5
                data:
                  title: Chore Completed
                  message: >-
                    Child5 has completed {{ states('input_text.child5_chore_25_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Awaiting response for Chore 
        wait_for_trigger:
          - platform: event
            event_type: mobile_app_notification_action
            event_data:
              action: "{{ action_confirm }}"
          - platform: event
            event_type: mobile_app_notification_action
            event_data:
              action: "{{ action_dismiss }}"
        timeout: "00:15:00"
        continue_on_timeout: true

      - alias: Process response for Chore 
        choose:
          - conditions:
              - condition: template
                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_confirm }}"
            sequence:
              - alias: Increment counter for Chore 25
                repeat:
                  count: "{{ states('input_number.child5_chore_25_chore_value') | int }}"
                  sequence:
                    - service: counter.increment
                      target:
                        entity_id: counter.child5_counter

              - alias: Mark Chore 
                service: input_boolean.turn_on
                target:
                  entity_id: input_boolean.child5_chore_25_completed

          - conditions:
              - condition: template
                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_dismiss }}"
            sequence:
              - alias: Clear pending notification for Chore 25
                service: input_boolean.turn_off
                target:
                  entity_id: input_boolean.child5_chore_25_pending

          - conditions:
              - condition: template
                value_template: "{{ wait.trigger is none }}"
            sequence:
              - alias: Trigger helper script on timeout
                service: script.turn_on
                target:
                  entity_id: script.child5_chore_25_pending_completion_helper

