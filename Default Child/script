    sequence:
      - alias: stop main script
        service: script.turn_off
        target:
          entity_id: script.child1_reward_01_pending_completion

      - alias: Wait for 1 second
        delay: "00:00:01"

      - alias: Reload main script
        service: script.turn_on
        target:
          entity_id: script.child1_reward_01_pending_completion

