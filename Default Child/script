      - alias: Conditionally Send Notification for Chore 85 Device 1
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_1_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 85 to Device 1
                service: notify.device_1
                data:
                  title: Chore Completed
                  message: >-
                    Child1 has completed {{ states('input_text.child1_chore_85_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 85 Device 2
