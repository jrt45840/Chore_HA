        variables:
          action_confirm: "{{ 'CONFIRM_' ~ context.id }}"
          action_dismiss: "{{ 'DISMISS_' ~ context.id }}"

      - alias: Mark notification for Chore 55 as pending
        service: input_boolean.turn_on
        target:
          entity_id: input_boolean.child3_chore_55_pending

      - alias: Conditionally Send Notification for Chore 55 Device 1
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_1_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 55 to Device 1
                service: notify.device_1
                data:
                  title: Chore Completed
                  message: >-
                    Child3 has completed {{ states('input_text.child3_chore_55_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 55 Device 2
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_2_enabled
                state: "on"
