                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 69 Device 5
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_5_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 69 to Device 5
                service: notify.device_5
                data:
                  title: Chore Completed
                  message: >-
                    Child1 has completed {{ states('input_text.child1_chore_69_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Awaiting response for Chore 
        wait_for_trigger:
          - platform: event
            event_type: mobile_app_notification_action
            event_data:
              action: "{{ action_confirm }}"
          - platform: event
            event_type: mobile_app_notification_action
            event_data:
              action: "{{ action_dismiss }}"
        timeout: "00:15:00"
        continue_on_timeout: true

      - alias: Process response for Chore 
        choose:
          - conditions:
              - condition: template
                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_confirm }}"
            sequence:
              - alias: Increment counter for Chore 69
                repeat:
                  count: "{{ states('input_number.child1_chore_69_chore_value') | int }}"
                  sequence:
                    - service: counter.increment
                      target:
                        entity_id: counter.child1_counter

              - alias: Mark Chore 
                service: input_boolean.turn_on
                target:
                  entity_id: input_boolean.child1_chore_69_completed

          - conditions:
              - condition: template
                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_dismiss }}"
            sequence:
              - alias: Clear pending notification for Chore 69
                service: input_boolean.turn_off
                target:
                  entity_id: input_boolean.child1_chore_69_pending

          - conditions:
              - condition: template
                value_template: "{{ wait.trigger is none }}"
            sequence:
              - alias: Trigger helper script on timeout
                service: script.turn_on
                target:
                  entity_id: script.child1_chore_69_pending_completion_helper

