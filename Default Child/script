        service: input_boolean.turn_on
        target:
          entity_id: input_boolean.child3_chore_97_pending

      - alias: Conditionally Send Notification for Chore 97 Device 1
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_1_enabled
                state: "on"
            sequence:
              - alias: Send notification for Chore 97 to Device 1
                service: notify.device_1
                data:
                  title: Chore Completed
                  message: >-
                    Child3 has completed {{ states('input_text.child3_chore_97_chore') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Chore 97 Device 2
        choose:
