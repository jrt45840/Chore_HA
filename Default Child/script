        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_1_enabled
                state: "on"
            sequence:
              - alias: Send notification for Reward 18 to Device 1
                service: notify.device_1
                data:
                  title: Reward Claimed
                  message: >-
                    Child4 has claimed {{ states('input_text.child4_reward_18_reward') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

