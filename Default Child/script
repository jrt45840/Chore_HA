          action_confirm: "{{ 'CONFIRM_' ~ context.id }}"
          action_dismiss: "{{ 'DISMISS_' ~ context.id }}"

      - alias: Mark notification for Reward 15 as pending
        service: input_boolean.turn_on
        target:
          entity_id: input_boolean.child1_reward_15_pending

      - alias: Conditionally Send Notification for Reward 15 Device 1
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_1_enabled
                state: "on"
            sequence:
              - alias: Send notification for Reward 15 to Device 1
                service: notify.device_1
                data:
                  title: Reward Claimed
                  message: >-
                    Child1 has claimed {{ states('input_text.child1_reward_15_reward') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Reward 15 Device 2
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_2_enabled
                state: "on"
            sequence:
              - alias: Send notification for Reward 15 to Device 2
                service: notify.device_2
                data:
                  title: Reward Claimed
                  message: >-
                    Child1 has claimed {{ states('input_text.child1_reward_15_reward') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Reward 15 Device 3
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_3_enabled
                state: "on"
            sequence:
              - alias: Send notification for Reward 15 to Device 3
                service: notify.device_3
                data:
                  title: Reward Claimed
                  message: >-
                    Child1 has claimed {{ states('input_text.child1_reward_15_reward') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Reward 15 Device 4
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_4_enabled
                state: "on"
            sequence:
              - alias: Send notification for Reward 15 to Device 4
                service: notify.device_4
                data:
                  title: Reward Claimed
                  message: >-
                    Child1 has claimed {{ states('input_text.child1_reward_15_reward') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Conditionally Send Notification for Reward 15 Device 5
        choose:
          - conditions:
              - condition: state
                entity_id: input_boolean.notify_5_enabled
                state: "on"
            sequence:
              - alias: Send notification for Reward 15 to Device 5
                service: notify.device_5
                data:
                  title: Reward Claimed
                  message: >-
                    Child1 has claimed {{ states('input_text.child1_reward_15_reward') }}
                  data:
                    actions:
                      - action: "{{ action_confirm }}"
                        title: Confirm
                      - action: "{{ action_dismiss }}"
                        title: Dismiss

      - alias: Awaiting response for Reward 
        wait_for_trigger:
          - platform: event
            event_type: mobile_app_notification_action
            event_data:
              action: "{{ action_confirm }}"
          - platform: event
            event_type: mobile_app_notification_action
            event_data:
              action: "{{ action_dismiss }}"
        timeout: "00:15:00"
        continue_on_timeout: true

      - alias: Process response for Reward 
        choose:
          - conditions:
              - condition: template
                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_confirm }}"
            sequence:
              - alias: Increment counter for Chore 15
                repeat:
                  count: "{{ states('input_number.child1_reward_15_reward_value') | int }}"
                  sequence:
                    - service: counter.increment
                      target:
                        entity_id: counter.child1_counter

              - alias: Mark Chore 
                service: input_boolean.turn_on
                target:
                  entity_id: input_boolean.child1_reward_15_claimed

          - conditions:
              - condition: template
                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_dismiss }}"
            sequence:
              - alias: Clear pending notification for Reward 15
                service: input_boolean.turn_off
                target:
                  entity_id: input_boolean.child1_reward_15_pending

          - conditions:
              - condition: template
                value_template: "{{ wait.trigger is none }}"
            sequence:
              - alias: Trigger helper script on timeout
                service: script.turn_on
                target:
                  entity_id: script.child1_reward_15_pending_completion_helper

