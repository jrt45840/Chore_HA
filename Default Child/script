                value_template: "{{ wait.trigger and wait.trigger.event.data.action == action_confirm }}"
            sequence:
              - alias: Increment counter for Chore 58
                repeat:
                  count: "{{ states('input_number.child1_chore_58_chore_value') | int }}"
